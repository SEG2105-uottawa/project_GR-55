<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HourSelect">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="Select Availability"
        android:textAppearance="@android:style/TextAppearance.Material.Headline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="lollipop" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/editTextWedStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchWednesday"
            app:layout_constraintTop_toBottomOf="@+id/editTextTuesStart" />

        <TextView
            android:id="@+id/editTextWedEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/editTextWedStart"
            app:layout_constraintTop_toBottomOf="@+id/editTextTuesEnd" />

        <Switch
            android:id="@+id/switchMonday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:checked="false"
            android:text="Monday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:checked="false"
            tools:ignore="UseSwitchCompatOrMaterialXml" />

        <TextView
            android:id="@+id/editTextMonStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="52dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchMonday"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/editTextMonEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="52dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/editTextMonStart"
            app:layout_constraintTop_toTopOf="parent" />

        <Switch
            android:id="@+id/switchTuesday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Tuesday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switchMonday"
            tools:ignore="UseSwitchCompatOrMaterialXml"/>

        <TextView
            android:id="@+id/editTextTuesStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchTuesday"
            app:layout_constraintTop_toBottomOf="@+id/editTextMonStart" />

        <TextView
            android:id="@+id/editTextTuesEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/editTextTuesStart"
            app:layout_constraintTop_toBottomOf="@+id/editTextMonEnd" />

        <Switch
            android:id="@+id/switchWednesday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Wednesday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switchTuesday"
            tools:ignore="UseSwitchCompatOrMaterialXml"/>

        <TextView
            android:id="@+id/editTextThursStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchThursday"
            app:layout_constraintTop_toBottomOf="@+id/editTextWedStart" />

        <TextView
            android:id="@+id/editTextThursEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/editTextThursStart"
            app:layout_constraintTop_toBottomOf="@+id/editTextWedEnd" />

        <Switch
            android:id="@+id/switchThursday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Thursday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switchWednesday"
            tools:ignore="UseSwitchCompatOrMaterialXml"/>

        <Switch
            android:id="@+id/switchFriday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Friday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switchThursday"
            tools:ignore="UseSwitchCompatOrMaterialXml"/>

        <TextView
            android:id="@+id/editTextFriStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchFriday"
            app:layout_constraintTop_toBottomOf="@+id/editTextThursStart" />

        <TextView
            android:id="@+id/editTextFriEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/editTextFriStart"
            app:layout_constraintTop_toBottomOf="@+id/editTextThursEnd" />

        <TextView
            android:id="@+id/editTextSatStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchSaturday"
            app:layout_constraintTop_toBottomOf="@+id/editTextFriStart" />

        <TextView
            android:id="@+id/editTextSatEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:visibility="visible"
            app:layout_constraintStart_toEndOf="@+id/editTextSatStart"
            app:layout_constraintTop_toBottomOf="@+id/editTextFriEnd" />

        <Switch
            android:id="@+id/switchSaturday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Saturday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switchFriday"
            tools:ignore="UseSwitchCompatOrMaterialXml"/>

        <Switch
            android:id="@+id/switchSunday"
            android:layout_width="120dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"
            android:text="Sunday"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.032"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/switchSaturday"
            tools:ignore="UseSwitchCompatOrMaterialXml"/>

        <TextView
            android:id="@+id/editTextSunStart"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            app:layout_constraintStart_toEndOf="@+id/switchSunday"
            app:layout_constraintTop_toBottomOf="@+id/editTextSatStart" />

        <TextView
            android:id="@+id/editTextSunEnd"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:ems="10"
            android:inputType="time"
            android:selectAllOnFocus="false"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editTextSunStart"
            app:layout_constraintTop_toBottomOf="@+id/editTextSatEnd"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/SubmitTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Submit"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.875"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextSunEnd" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>